<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN" "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="DB2Tables"  defaultModelType="flat"  targetRuntime="MyBatis3">
		<!-- Mybatis generator mapper文件重新生成不会覆盖原文件，为了解决此问题 -->
		<plugin type="OverIsMergeablePlugin"></plugin>
	
		<!-- 是否去除自动生成的注释 -->
		<commentGenerator>
			<property name="suppressAllComments" value="true"/>
			<property name="suppressDate" value="true"/>
		</commentGenerator>

		<!-- 数据库连接信息 -->
		<jdbcConnection driverClass="com.mysql.jdbc.Driver"	connectionURL="jdbc:mysql://192.168.1.107:3306/js" userId="news" password="123456">
		</jdbcConnection>
		<javaTypeResolver>
			<property name="forceBigDecimals" value="true"/>
		</javaTypeResolver>

		<!-- po实体类代码位置 -->
		<javaModelGenerator targetPackage="com.jxzc.model.entity"	targetProject="D:\\development\\workspace\\jsjz\\src\\main\\java">
			<property name="enableSubPackages" value="true" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<!-- sql代码的xml映射文件位置 -->
		<sqlMapGenerator targetPackage="mapper" targetProject="D:\\development\\workspace\\jsjz\\src\\main\\resources">
			<property name="enableSubPackages" value="true" />
		</sqlMapGenerator>
		<!-- mapper接口位置 -->
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.jxzc.model.dao" targetProject="D:\\development\\workspace\\jsjz\\src\\main\\java">
			<property name="enableSubPackages" value="true" />
		</javaClientGenerator>

		<!-- 需要自动生成的表 -->
		<!--<table tableName="car_driver_team" domainObjectName="CarDriverTeam"-->
			<!--enableSelectByExample="false" enableDeleteByExample="false"-->
			<!--enableCountByExample="false" enableUpdateByExample="false">-->
			<!--<property name="useActualColumnNames" value="false" />-->
			<!--<generatedKey column="id" sqlStatement="MySql" identity="true" />-->
		<!--</table>-->

		<table tableName="driver_daily_report" domainObjectName="DriverDailyReport"
			   enableSelectByExample="false" enableDeleteByExample="false"
			   enableCountByExample="false" enableUpdateByExample="false">
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
		</table>

		<table tableName="car_relate_group" domainObjectName="CarRelateGroup"
			   enableSelectByExample="false" enableDeleteByExample="false"
			   enableCountByExample="false" enableUpdateByExample="false">
			<property name="useActualColumnNames" value="false" />
			<generatedKey column="id" sqlStatement="MySql" identity="true" />
		</table>

	</context>
</generatorConfiguration>